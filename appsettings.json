{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RangoDbStr": "Data Source=rango.db"
  },
  "AllowedHosts": "*",
  "Authentication": {
    "DefaultScheme": "Bearer",
    "Scheme": {
      "Bearer": {
        "ValidAudiences": [
          "CursoAsp"
        ],
        "ValidIssuer": "dotnet-user-jwts"
        //ISS do token, consultado no https://jwt.io/
      }
    }
  }
}
